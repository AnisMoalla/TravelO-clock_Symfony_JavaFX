<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.Rating?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GestionEvents.gui.ReservationTouristeController">
    <children>
      <Label layoutX="330.0" layoutY="14.0" prefHeight="105.0" prefWidth="679.0" text="Faire une reservation">
         <font>
            <Font size="72.0" />
         </font>
      </Label>
      <TableView fx:id="tableViewOffre" layoutY="142.0" prefHeight="496.0" prefWidth="1280.0">
        <columns>
          <TableColumn fx:id="colonneEvent" prefWidth="272.0" text="Nom Event" />
            <TableColumn fx:id="colonneOffre" prefWidth="344.0" text="Nom Offre" />
            <TableColumn fx:id="colonneDateDebut" prefWidth="216.0" text="Date Debut" />
            <TableColumn fx:id="colonneDateFin" prefWidth="223.0" text="Date Fin" />
            <TableColumn fx:id="colonnePourcentage" prefWidth="221.0" text="Pourcentage" />
            <TableColumn fx:id="coloneIdEvent" minWidth="0.0" prefWidth="0.0" text="ColumnX" />
            <TableColumn fx:id="coloneIdOffre" minWidth="0.0" prefWidth="0.0" text="Column X" />
        </columns>
      </TableView>
      <Button fx:id="boutonRetour" layoutX="1180.0" layoutY="644.0" mnemonicParsing="false" onAction="#actionRetour" text="Retour" />
      <Rating fx:id="rating" layoutX="300.0" layoutY="644.0" />
      <Button fx:id="boutonVoter" layoutX="504.0" layoutY="644.0" mnemonicParsing="false" onAction="#actionVoter" text="Voter" />
   </children>
    
</AnchorPane>
